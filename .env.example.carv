# Environment Variables Template
# Copy this to .env for local development

# Telegram Bot
BOT_TOKEN=123456:ABC-DEF1234ghIkl-zyx57W2v1u123ew11
OWNER_TELEGRAM_ID=123456789

# Supabase (existing)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key

# AI (existing)
GEMINI_API_KEY=your-gemini-api-key

# CARV ID OAuth
CARV_CLIENT_ID=your-carv-client-id
CARV_CLIENT_SECRET=your-carv-client-secret
CARV_REDIRECT_URL=https://your-worker.workers.dev/auth/carv/callback

# Optional: Override default CARV endpoints
# CARV_AUTH_URL=https://protocol.carv.io/oauth/authorize
# CARV_TOKEN_URL=https://protocol.carv.io/oauth/token
# CARV_USERINFO_URL=https://protocol.carv.io/oauth/userinfo

# Encryption Secret (32-byte hex string)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_SECRET=0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef
